{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true,
        "jasmine": true,
        "codeceptjs/codeceptjs": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-underscore-dangle": 0,
        "no-tabs": 0,
        "no-console": 0,
        "no-plusplus": 0,
        "import/prefer-default-export": "off"
    },
    "plugins": ["jasmine", "codeceptjs"]
}
